{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./index.js",
      "./lib/"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./out/doc",
    "package": "./package.json",
    "readme": "./README.md",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
